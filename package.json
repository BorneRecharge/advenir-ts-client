{
  "name": "@bornerecharge/advenir-ts-client",
  "version": "0.0.0-development",
  "description": "A client to send EV charging sessions to Advenir",
  "license": "MIT",
  "author": "Borne Recharge Service <technical@bornerecharge.fr> (https://github.com/BorneRecharge)",
  "homepage": "https://github.com/BorneRecharge/advenir-ts-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BorneRecharge/advenir-ts-client.git"
  },
  "bugs": {
    "url": "https://github.com/BorneRecharge/advenir-ts-client/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./types": {
      "types": "./dist/src/lib/types.d.ts",
      "default": "./dist/src/lib/types.js"
    }
  },
  "files": [
    "dist/src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./dist/",
    "cm": "cz",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-package": "prettier-package-json --write ./package.json",
    "lint": "eslint ./src/ --fix",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-package-json": "^2.8.0",
    "semantic-release": "^23.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  },
  "keywords": [
    "advenir",
    "avere",
    "electric-vehicle",
    "ev"
  ],
  "engines": {
    "node": ">=20.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --cache-location .eslintcache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "build",
              "scope": "deps",
              "release": "patch"
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Bug Fixes"
              },
              {
                "type": "build",
                "section": "Dependencies and Other Build Updates",
                "hidden": false
              }
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
